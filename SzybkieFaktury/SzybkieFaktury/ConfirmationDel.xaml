<Window x:Class="SzybkieFaktury.ConfirmationDel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SzybkieFaktury"
        mc:Ignorable="d"
        Title="ConfirmationDel" Height="450" Width="800">
    <Grid>
        <Label Content="Jesteś pewien, że chcesz usunąć firmę:&#xD;&#xA;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="DeleteName" Content="Label" HorizontalAlignment="Left" Margin="10,52,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.676,-0.365"/>
        <Label x:Name="DeleteNIP" Content="Label" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top"/>
        <Button x:Name="DeleteConfirmed" Content="Tak, jestem pewien" HorizontalAlignment="Left" Margin="81,161,0,0" VerticalAlignment="Top" Width="121" Click="DeleteConfirmed_Click"/>
        <Button x:Name="DeleteAbort" Content="Anuluj" HorizontalAlignment="Left" Margin="216,161,0,0" VerticalAlignment="Top" Width="75" Click="DeleteAbort_Click"/>

    </Grid>
</Window>
